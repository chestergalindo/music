{
  "name": "musiclist",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint --ignore-pattern .gitignore --ext .js,.ts,.tsx './src'",
    "prettier": "prettier  --ignore-pattern .gitignore --ext .js,.ts,.tsx,.json './src'",
    "check-format": " npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel check-types check-format",
    "dev": "vite",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@nextui-org/react": "1.0.0-beta.12",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@types/react-portal": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "@vitejs/plugin-react": "^3.0.0",
    "axios": "1.2.2",
    "babel-preset-react-app": "10.0.1",
    "enzyme": "3.11.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.2.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-runner-eslint": "1.1.0",
    "lint-staged": "13.1.0",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "react-content-loader": "6.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.7.1",
    "react-portal": "4.2.2",
    "react-query": "3.39.2",
    "react-router-dom": "6.6.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
